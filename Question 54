#include <stdio.h>
int main(){
 // making a fixed repeating pattern 
/* 1
   1 2
   1 2 3
and so on */
 int n;
 printf("enter the number of times you want to print the pattern ");
 scanf("%d",&n);
 for(int i=1;i<=n;i++){
    for(int j=1;j<=i;j++){
        printf("%d ",j);
    }
    printf("\n");
 }
   return 0;
}
