#include <stdio.h>
int main(){
   // plotting a patternn of rows=number and its frequency
   int n;//for frequency
   printf("enter a number ");
   scanf("%d",&n);
   for(int row=1;row<=n;row++){
      for(int col=1;col<=row;col++){
         printf("%d ",row);
        
      }
      printf("\n");
   }
   return 0;
}
