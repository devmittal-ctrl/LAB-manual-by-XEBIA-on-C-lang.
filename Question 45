#include <stdio.h>
#include <stdlib.h>
int main(){
   int n,m,max=1;
   printf("enter the number in which you have to practise ");
   scanf("%d",&n);
   // finding the largest nimber from n user inputs
   for(int i=1;i<=n;i++){
      printf("the %d number is ",i);
      scanf("%d",&m);
      if(max>=m){
         max=1;
      }
      else {
         max=m;
      }
   }
   printf("the largest number among them is %d ",max);
      return 0;
}
